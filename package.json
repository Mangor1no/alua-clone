{
  "name": "eslint-prettier-setup",
  "version": "1.0.0",
  "description": "Boilerplate for projects",
  "main": "index.js",
  "author": "Mangorino",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "webpack serve --mode development --open --config webpack.dev.js",
    "build": "set NODE_ENV=production&& webpack --config webpack.prod.js --mode production",
    "build-w": "set NODE_ENV=development&& webpack --config webpack.prod.js --mode production --watch --progress",
    "preview": "http-server dist"
  },
  "dependencies": {
    "@babel/runtime": "7.13.10",
    "axios": "0.21.1",
    "framer-motion": "^4.1.0",
    "masonic": "^3.4.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-redux": "7.2.2",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "tailwindcss": "2.0.4"
  },
  "devDependencies": {
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@babel/eslint-parser": "7.13.10",
    "@babel/eslint-plugin": "7.13.10",
    "@babel/plugin-transform-runtime": "7.13.10",
    "@babel/preset-env": "7.13.10",
    "@babel/preset-react": "7.12.13",
    "@fortawesome/fontawesome-svg-core": "1.2.35",
    "@fortawesome/free-solid-svg-icons": "5.15.3",
    "@fortawesome/react-fontawesome": "0.1.14",
    "autoprefixer": "10.2.5",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "4.1.0",
    "chalk": "4.1.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "5.1.2",
    "eslint": "7.21.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-import-resolver-babel-module": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.2.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.3.1",
    "http-server": "0.12.3",
    "husky": "=4",
    "lint-staged": "10.5.4",
    "mini-css-extract-plugin": "1.3.9",
    "postcss": "8.2.8",
    "postcss-import": "14.0.0",
    "postcss-loader": "5.2.0",
    "prettier": "2.2.1",
    "progress-bar-webpack-plugin": "2.1.0",
    "redux-devtools-extension": "2.13.9",
    "sass": "1.32.8",
    "sass-loader": "11.0.1",
    "style-loader": "2.0.0",
    "swiper": "6.5.2",
    "webpack": "5.24.4",
    "webpack-cli": "4.5.0",
    "webpack-dev-server": "3.11.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}

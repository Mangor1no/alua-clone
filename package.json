    "@babel/runtime": "7.13.10",
    "autoprefixer": "10.2.5",
    "babel-loader": "8.2.2",
    "babel-plugin-module-resolver": "4.1.0",
    "chalk": "4.1.0",
    "clean-webpack-plugin": "3.0.0",
    "css-loader": "5.1.2",
    "css-minimizer-webpack-plugin": "^1.3.0",
    "eslint": "7.21.0",
    "eslint-config-airbnb": "18.2.1",

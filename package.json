    "react-icons": "^4.2.0",
    "react-redux": "7.2.2",
    "react-responsive-masonry": "^2.1.2",
    "react-router-dom": "^5.2.0",
    "react-virtualized": "^9.22.3",
    "react-virtualized-image-measurer": "^0.4.0",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "swiper": "6.5.2",
